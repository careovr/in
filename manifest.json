{
  "name": "Careovr",
  "short_name": "Careovr",
  "description": "India's Fastest, Trusted and Affordable Healthcare Platform",
  "start_url": "/index.html",
  "display": "standalone",
  "background_color": "#F8F9FA",
  "theme_color": "#F8F9FA",
  "icons": [
    {
      "src": "img/logo.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "img/logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "splash_screen": {
    "src": "https://careovr.netlify.app/img/logo.png",
    "sizes": "512x512",
    "type": "image/png",
    "purpose": "maskable"
  },
  "id": "/",
  "orientation": "portrait",
  "screenshots": [
    {
      "src": "img/shots/shot1.png",
      "sizes": "932x430",
      "type": "image/png"
    },
    {
      "src": "img/shots/shot2.png",
      "sizes": "932x430",
      "type": "image/png"
    }
  ],
  "display_override": [
    "standalone",
    "fullscreen",
    "window-controls-overlay"
  ],
  "categories": [
    "health",
    "fitness",
    "medical",
    "lifestyle"
  ],
  "dir": "ltr",
  "lang": "en",
  "scope": "/",
  "launch_handler": {},
  "handle_links": {
    "scope": "/",
    "urls": [
      "/",
      "/index.html"
    ]
  },
  "protocol_handlers": [
    {
      "protocol": "web+careover",
      "url": "/?url=%s"
    }
  ],
  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "description",
      "url": "link",
      "files": [
        {
          "name": "",
          "accept": []
        }
      ]
    }
  },
  "background_sync": true,
  "periodic_sync": {
    "min_interval": 86400
  },
  "prefer_related_applications": false,
  "version": "1.13.2.9.11.1",
  "edge_side_panel": {
    "preferred_width": 480
  },
  "file_handlers": [
    {
      "action": "/open-report",
      "accept": {
        "application/pdf": [
          ".pdf"
        ],
        "text/csv": [
          ".csv"
        ]
      }
    }
  ],
  "shortcuts": [
    {
      "name": "Book Ambulance",
      "url": "https://careovr.netlify.app/ambulance",
      "icons": [
        {
          "src": "img/logo.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Refer & Earn",
      "url": "https://careovr.netlify.app/refer",
      "icons": [
        {
          "src": "img/logo.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "gcm_sender_id": "850234327014",
  "scope_extensions": [
    {
      "origin": "https://careovr.netlify.app/join"
    }
  ]
}
